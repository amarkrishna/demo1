// This example script trains a Weka-based model on the 
//  formation energies of a small set of compounds

// Load in a dataset of compounds
data = new data.materials.CompositionDataset
data import ./datasets/small_set.txt

// Define where to find elemental property data
data attributes properties directory ./Lookup Data/
// Select which set of elemental properties to use for attributes
data attributes properties add set general

// Generate new attributes
data attributes generate

// Create a multi-objective model. Will use two separate models to compute
//  volume_pa and delta_e
model = new models.regression.MultiObjectiveRegression PropertyFormulaRanker $data #{volume_pa} * #{delta_e}
leaf = new models.regression.WekaRegression trees.REPTree
model submodel set generic $leaf
model train $data
model crossvalidate $data 10

// Print out training statistics for whole model
print model validation stats

// Print out stats for delta_e model
leaf = model submodel get delta_e
print leaf training stats
exit
